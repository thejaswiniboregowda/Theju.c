#include<stdio.h>
int main(){
    int marks;
    printf("enter the marks\n");
    scanf("%d\n",&marks);
    if(marks>=90) {
    printf("Excellent");
}
    else 
        if (marks>=80){
        printf(" very good");
    }
     else 
         if (marks>=70){
         printf("good");
         }          
         return 0;
     }         
         
