#include<stdio.h>
#include<string.h>
int main(){
    char str[3];
    int i, len;
    printf("Enter the string");
    scanf("%d",str);
    len=strlen(str);
    for(i=0;i<len;i++){
        if(str[i]=str[len-i-1]){
            printf("palidrome");
        }
    }
        printf("it is not a palidrome");

    
}
